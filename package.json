{
  "name": "fe-chrome-extensions",
  "private": true,
  "version": "0.2.7",
  "type": "module",
  "scripts": {
    "dev:popup": "vite --mode development -c ./script/vite.popup.config.js",
    "dev:sidepanel": "vite --mode development -c ./script/vite.sidepanel.config.js",
    "build": "vite build -c ./script/vite.background.config.js && vite build -c ./script/vite.content.config.js && vite build -c ./script/vite.sidepanel.config.js",
    "prebuild": "rm -rf _build_* && rm -rf build",
    "postbuild": "node ./script/build.js",
    "deploy": "yarn build && node ./script/deploy.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview -c ./script/vite.popup.config.js"
  },
  "dependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@radix-ui/react-slot": "^1.2.3",
    "antd": "^5.25.1",
    "autoprefixer": "^10.4.17",
    "blueimp-md5": "^2.19.0",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.10",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.515.0",
    "parse5": "^7.3.0",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "6.17.0",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^3.1.0",
    "swiper": "^11.0.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/blueimp-md5": "^2.18.2",
    "@types/chrome": "^0.0.318",
    "@types/lodash": "^4.17.17",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/redux-logger": "^3.0.13",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "less": "^4.2.0",
    "prettier": "^3.1",
    "prettier-plugin-tailwindcss": "0.5.10",
    "typescript": "^5.3.3",
    "vite": "^4.5.3"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
